{
  "name": "aria-language",
  "displayName": "Aria Language",
  "description": "Syntax highlighting for the Aria programming language",
  "version": "0.1.0",
  "publisher": "aria",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "aria",
        "aliases": [
          "Aria",
          "aria"
        ],
        "extensions": [
          ".aria"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "aria",
        "scopeName": "source.aria",
        "path": "./syntaxes/aria.tmLanguage.json"
      }
    ]
  }
}
